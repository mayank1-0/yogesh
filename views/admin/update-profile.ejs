  <script>
    $(window).on('load', function () {
      console.log('Page loading successful');
      fetchAdminDetails();
      $('#update_profile').click(updateProfile);
      $('#admin-signout').click(logout);
    });

    function logout() {
        $.ajax({
          type: "GET",
          url: "/api/admin/logout",
          success: function success(result) {
            window.location.href = '/';
          },
          error: function error(error) {
            console.log(error, 'error');
          }
        })
      }

    function fetchAdminDetails() {
      $.ajax({
        type: "GET",
        url: "/api/admin/details",
        success: function success(result) {
          let adminData = result.admin;
          if (adminData) {
            // Set placeholder values in the input fields
            $("#hospital-name").attr("placeholder", adminData.name || 'Enter hospital name');
            $("#contact-number").attr("placeholder", adminData.mobile || 'Contact Number');
            // $("#contact-person-name").attr("placeholder", adminData.contactPerson || 'Contact person');
            // $("#district").attr("placeholder", adminData.district || 'District');
            // $("#pincode").attr("placeholder", adminData.pincode || 'Enter Pincode');
            // $("#state").attr("placeholder", adminData.state || 'State');
            // $("#designation").attr("placeholder", adminData.designation || 'Designation');
            // $("#department").attr("placeholder", adminData.department || 'Department');
          } else {
            console.log('No admin data found');
          }
        },
        error: function error(error) {
          console.log(error, 'error');
        }
      })
    }

    function updateProfile() {
      var data = {
        // Get values of inputs by their IDs
        name: $('#hospital-name').val(),
        mobile: $('#contact-number').val(),
      }
      console.log(data);
      $.ajax({
        type: "POST",
        url: "/api/admin/update-details",
        dataType: 'JSON',
        data: data,
        success: function success(result) {
          console.log('result', result);
          window.alert('Admin details update successful');
          window.location.reload(true);
        },
        error: function error(error) {
          console.log(error, 'error');
          window.alert(`${error.responseJSON.message}`);
        }
      })
    }

  </script>
