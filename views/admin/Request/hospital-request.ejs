  <script>
    $(window).on('load', function () {
      console.log('Page loaded successful');
      getAllHospitalRequest();
      $('#submitHospitalRequestByAdminButton').click(saveHospitalRequestByAdmin);
      $('#admin-signout').click(logout)
    });

    function logout() {
      $.ajax({
        type: "GET",
        url: "/api/admin/logout",
        success: function success(result) {
          window.location.href = '/';
        },
        error: function error(error) {
          console.log(error, 'error');
        }
      })
    }

    function saveHospitalRequestByAdmin() {
      var data = {
        // Get values of inputs by their IDs
        name: $('#hospital-name').val(),
        contactPerson: $('#contact-person-name').val(),
        email: $('#email-address').val(),
        contactNumber: $('#contact-number').val(),
        address: $('#address').val(),
        district: $('#district').val(),
        pincode: $('#pincode').val(),
        state: $('#state').val(),
        password: `${$('#hospital-name').val()}12345678`
      }
      console.log(data);
      $.ajax({
        type: "POST",
        url: "/api/hospital/sign-up",
        dataType: 'JSON',
        data: data,
        success: function success(result) {
          console.log('result', result);
          window.alert('Hospital request sign-up successful');
          window.location.reload(true);
        },
        error: function error(error) {
          console.log(error, 'error');
          window.alert(`${error.responseJSON.message}`);
        }
      })
    };

    function getAllHospitalRequest() {
      $.ajax({
        type: "GET",
        url: "/api/admin/getAllHospitalsRequest",
        // data: { name: search },
        success: function success(result) {
          let hospitalsArray = result.hospitals;
          $("#hospital_request_table").html("");
          hospitalsArray.forEach((i, index) => {
            $("#hospital_request_table").append(`
                    <tr>
                        <th scope="row"> ${index + 1} </th>
                        <td> ${i.name} </td>
                        <td> ${i.email} </td>
                        <td> ${i.password} </td>
                        <td> ${i.address} ${i.district} ${i.state} </td>
                        <td> ${i.contactNumber} </td>
                        <td> ${i.contactPerson} </td>
                        <td>
                        <span><a href="#" class="response-link" data-id="${i._id}" data-response="yes">Yes</a></span> / 
                        <span><a href="#" class="response-link" data-id="${i._id}" data-response="no">No</a></span>
                        </td>
                      </tr>
                `)
          });
          // Attach click event to the response links
          $('.response-link').on('click', function (e) {
            e.preventDefault(); // Prevent default link behavior
            const hospitalId = $(this).data('id');
            const response = $(this).data('response');  // either "yes" either "no"
            handleResponse(hospitalId, response);
          });
        },
        error: function error(error) {
          console.log(error, 'error');
        }
      })
    }

    function handleResponse(hospitalId, response) {
      $.ajax({
        type: "PUT", // or "PUT" depending on your API
        url: `/api/admin/verify-unverify-hospital/${hospitalId}`, // Modify the URL as needed
        data: { response: response },
        success: function (result) {
          console.log('Response submitted successfully:', result);
          // Optionally refresh the hospital request table or provide feedback to the user
          getAllHospitalRequest();
        },
        error: function (error) {
          console.log(error, 'error');
        }
      });
    }
  </script>
