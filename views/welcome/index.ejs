    <script>
        // Variable to hold the type of login selected
        let loginType = '';

        // Function to set loginType based on the button clicked
        function setLoginType(type) {
            loginType = type;
            console.log(`Login type selected: ${loginType}`);
        }

        $(document).ready(function () {
            // Add event listeners to the buttons using jQuery
            $('#tpa-login').on('click', function () {
                setLoginType('tpa');
            });

            // $('#hospital-login').on('click', function () {
            //     setLoginType('hospital');
            // });

            $('#insurance-com-login').on('click', function () {
                setLoginType('insuranceCompany');
            });
        });

        $(window).on('load', function () {
            console.log('Page loaded successful');
            loadHospitalsAndExcludedByData();
            fetchInsuranceCompanyNamesData();
            fetchTPANamesData()
        });

        $("#tpaForm").submit((e) => {
            e.preventDefault();
            console.log(`TPA form submitted`);
            registerTPA();
        });

        $("#insuranceCompanyForm").submit((e) => {
            e.preventDefault();
            console.log(`InsuranceCompany form submitted`);
            registerInsuranceCompany();
        });

        $("#hospitalRequestForm").submit((e) => {
            e.preventDefault();
            console.log(`HospitalRequest form submitted`);
            registerHospital();
        });

        // Call the function when you want to fetch the data, e.g., before showing the modal
        // $('#hospitalRequestModal').on('show.bs.modal', function () {
        //     fetchAllTPAsNameId();
        //     fetchAllInsuranceCompaniesNameId();
        // });

        // Call the function when you want to fetch the data, e.g., before showing the modal

        $('#insuranceCompanyModal').on('show.bs.modal', function () {
            fetchAllHospitalsNameId();
        });

        $('#main-login-button').on('click', login);

        $("#hospital-search").change(function () {
            loadHospitalsAndExcludedByData();
        })

        function registerTPA() {
            console.log('Register TPA Btn Clicked');
            var excludedHospitals = $("#ExcludeHospitalbyTPA").val();
            var data = {
                name: $("#tpaName").val(),
                contactPerson: $("#tpaContactPerson").val(),
                email: $("#tpaEmail").val(),
                contactNumber: $("#tpaPhone").val(),
                department: $("#tpaDepartment").val(),
                password: $("#tpaPassword").val(),
                excludedHospitals: excludedHospitals && excludedHospitals.length ? excludedHospitals : [], // Convert to array

            }
            console.log(data);
            $.ajax({
                type: "POST",
                url: "/api/tpa/sign-up",
                dataType: 'JSON',
                data: data,
                success: function success(result) {
                    console.log('result', result);
                    window.alert('TPA signup successful');
                    window.location.href = '/';
                },
                error: function error(error) {
                    console.log(error, 'error');
                    window.alert(`${error.responseJSON.message}`);
                }
            })
        }

        function registerInsuranceCompany() {
            console.log('Register InsuranceCompany Btn Clicked');
            var excludedHospitals = $("#ExcludeHospitalbyInsuranceCompany").val();
            var data = {
                name: $("#insuranceCompanyName").val(),
                contactPerson: $("#insuranceContactPerson").val(),
                email: $("#insuranceCompanyEmail").val(),
                contactNumber: $("#insuranceCompanyPhone").val(),
                address: $("#insuranceAddress").val(),
                establishYear: $("#insuranceEstablishYear").val(),
                password: $("#insuranceCompanyPassword").val(),
                excludedHospitals: excludedHospitals && excludedHospitals.length ? excludedHospitals : [], // Convert to array
            }
            console.log(data);
            $.ajax({
                type: "POST",
                url: "/api/insuranceCompany/sign-up",
                dataType: 'JSON',
                data: data,
                success: function success(result) {
                    console.log('result', result);
                    window.alert('Insurance Company sign-up successful');
                    window.location.href = '/';
                },
                error: function error(error) {
                    console.log(error, 'error');
                    window.alert(`${error.responseJSON.message}`);
                }
            })
        }

        function registerHospital() {
            // var excludedTPAs = $("#ExcludedTPA").val();
            // var excludedInsuranceCompanies = $("#ExcludedInsuranceCompany").val();

            var data = {
                name: $("#hospitalName").val(),
                contactPerson: $("#hospitalContactPerson").val(),
                email: $("#hospitalEmail").val(),
                contactNumber: $("#hospitalPhone").val(),
                address: $("#hospitalAddress").val(),
                pincode: $("#hospitalPincode").val(),
                district: $("#hospitalDistrict").val(),
                state: $("#hospitalState").val(),
                password: $("#hospitalEmailPassword").val(),
                // excludedTPAs: excludedTPAs && excludedTPAs.length ? excludedTPAs : [], // Convert to array
                // excludedInsuranceCompanies: excludedInsuranceCompanies && excludedInsuranceCompanies.length ? excludedInsuranceCompanies : [] // Ensure it's an array
            }
            console.log(data);
            $.ajax({
                type: "POST",
                url: "/api/hospital/sign-up",
                dataType: 'JSON',
                data: data,
                success: function success(result) {
                    console.log('result', result);
                    window.alert('Hospital request sign-up successful');
                    window.location.href = '/';
                },
                error: function error(error) {
                    console.log(error, 'error');
                    window.alert(`${error.responseJSON.message}`);
                }
            })
        }

        function loadHospitalsAndExcludedByData() {
            let search = $('#hospital-search').val();
            $.ajax({
                type: "GET",
                url: "/api/welcome/getAllHospitalsAndExcludedByData",
                data: { name: search, isVerified: 'Approve' },
                success: function success(result) {
                    // let churan = result.hospitals;
                    let resultDisplay = result.items;
                    var tr = '';
                    $("#hospitalTable").html("");
                    $('#pagination').html(result.paginationHtml);
                    resultDisplay.forEach((i, index) => {
                        const excludedByTPAsExists = (i.excludedByTPAs && i.excludedByTPAs.length > 0) ||
                            (i.excludedByInsuranceCompanies && i.excludedByInsuranceCompanies.length > 0);
                        const clickHandler = excludedByTPAsExists ? `onclick="fetchExcludedByTPAsInsuranceCompanies('${i._id}')"` : '';
                        const excludedText = excludedByTPAsExists ? "Yes" : "No";
                        // Conditionally add modal attributes
                        const modalAttributes = excludedByTPAsExists ? `data-bs-toggle="modal" data-bs-target="#staticBackdrop"` : '';

                        $("#hospitalTable").append(`
                    <tr>
                        <th scope="row"> ${index + 1} </th>
                        <td> ${i.name} </td>
                        <td> ${i.contactNumber} </td>
                        <td> ${i.email} </td>
                        <td> ${i.address} </td>
                        <td> ${i.pincode} </td>
                        <td> ${i.district} </td>
                        <td> ${i.state} </td>
                        <td ${modalAttributes} ${clickHandler}> ${excludedText} </td>
                    </tr>
                `)
                    });
                },
                error: function error(error) {
                    console.log(error, 'error');
                }
            })
        }

        function fetchExcludedByTPAsInsuranceCompanies(id) {
            $.ajax({
                type: "GET",
                url: `/api/welcome/excludedByInsuranceAndTPA/${id}`,
                success: function success(result) {
                    const name = result.hospitalName;
                    let excludedByInsuranceCompanies = result.excludedByInsuranceCompanies.slice(0, 5);
                    let excludedByTPAs = result.excludedByTPAs.slice(0, 5);
                    $("#staticBackdropLabel").text(name);
                    // Populate excluded insurance companies
                    $('#insurance-list').html("<li> Excluded by Insurance company </li>");
                    excludedByInsuranceCompanies.forEach(function (company) {
                        $('#insurance-list').append(`<li>${company.name}</li>`);
                    });

                    // Populate excluded TPAs
                    $('#tpa-list').html(`<li> Excluded by TPA </li>`);
                    excludedByTPAs.forEach(function (tpa) {
                        $('#tpa-list').append(`<li>${tpa.name}</li>`);
                    });
                },
                error: function error(error) {
                    console.log(error, 'error');
                }
            })
        }

        function fetchAllHospitalsNameId() {
            $.ajax({
                type: "GET",
                url: `/api/welcome/getAllHospitalsNameId`,
                success: function success(result) {
                    const hospitals = result.hospitals;
                    // Clear existing options in the dropdowns
                    // $('#ExcludedInsuranceCompany').empty().append('<option selected>Ex. Insurance Company</option>');

                    // Populate Excluded InsuranceCompanies Dropdown
                    hospitals.forEach(function (hospital) {
                        $('#ExcludeHospitalbyTPA').append(`<option value="${hospital._id}">${hospital.name}</option>`);
                        $('#ExcludeHospitalbyInsuranceCompany').append(`<option value="${hospital._id}">${hospital.name}</option>`);
                    });
                },
                error: function error(error) {
                    console.log(error, 'error');
                }
            })
        }

        function fetchTPANamesData() {
            $.ajax({
                type: "GET",
                url: `/api/welcome/getAllTPANamesIds`,
                success: function success(result) {
                    const tpaNameNames = result.tpaNameNames;
                    // Clear existing options in the dropdowns
                    $('#tpaName').empty();

                    // Populate TPA Name Dropdown
                    tpaNameNames.forEach(function (tpaNameName) {
                        $('#tpaName').append(`<option value="${tpaNameName._id}">${tpaNameName.name}</option>`);
                    });
                },
                error: function error(error) {
                    console.log(error, 'error');
                }
            })
        };

        function fetchInsuranceCompanyNamesData() {
            $.ajax({
                type: "GET",
                url: `/api/welcome/getAllInsuranceCompanyNamesIds`,
                success: function success(result) {
                    const insuranceCompanyNames = result.insuranceCompanyNames;
                    // Clear existing options in the dropdowns
                    $('#insuranceCompanyName').empty();

                    // Populate InsuranceCompanies Name Dropdown
                    insuranceCompanyNames.forEach(function (insuranceCompanyName) {
                        $('#insuranceCompanyName').append(`<option value="${insuranceCompanyName._id}">${insuranceCompanyName.name}</option>`);
                    });
                },
                error: function error(error) {
                    console.log(error, 'error');
                }
            })
        }

        // Function to handle login

        function login() {
            // Implement your login logic here
            if (!loginType) return window.alert('Please select a Login type');
            console.log(`Attempting to login as: ${loginType}`);

            const email = $('#loginUserId').val();
            const password = $('#loginPassword').val();
            const captcha = $('#captchaInput').val();

            // captha functionality goes here
            // ------------------------------

            const data = { email, password };

            if (!email || !password || !captcha) return window.alert('Please fill all the input fields');
            // Perform your login logic (e.g., AJAX call)
            console.log(`User ID: ${email}, Password: ${password}, Captcha: ${captcha}`);

            $.ajax({
                type: "POST",
                url: `/api/${loginType}/sign-in`,
                dataType: 'JSON',
                data: data,
                success: function success(result) {
                    console.log('result', result);
                    window.alert('Sign in success');
                    if (loginType == "insuranceCompany") window.location.href = "/insurance-company/dashboard"
                    else window.location.href = "/tpa/dashboard"
                },
                error: function error(error) {
                    console.log(error, 'error');
                    window.alert(`${error.responseJSON.message}`);
                }
            })

            // Example: Clear the modal after logging in
            $('#loginModal').modal('hide');
        }

    </script>
