<body>
    <!-- Login Modal Structure -->
    <div class="modal fade login-modal" id="loginModal" tabindex="-1" aria-labelledby="loginModalLabel"
        aria-hidden="true">
        <div class="modal-dialog modal-fullscreen-sm-down pop-up-main">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="btn btn-primary" id="tpa-login">TPA </button>
                    <button type="button" class="btn btn-primary" id="hospital-login">HOSPITAL </button>
                    <button type="button" class="btn btn-primary" id="insurance-com-login">INSURANCE COMPANY </button>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body pop-up-body">
                    <input class="form-control form-control-sm mb-3" type="text" id="loginUserId" placeholder="Email "
                        aria-label="User ID" required>
                    <input class="form-control form-control-sm mb-3" type="password" id="loginPassword"
                        placeholder="Password" aria-label="Password" required>

                    <div class="captcha-box mb-3">
                        <label for="captchaInput" class="form-label">Captcha</label>
                        <input class="form-control form-control-sm" type="text" id="captchaInput"
                            placeholder="Enter Captcha" aria-label="Captcha" required>
                        <input class="form-control form-control-sm mt-2" disabled type="text" value="ABCD1234"
                            aria-label="Captcha Value" readonly>
                        <div class="refresh-btn mt-2" style="cursor:pointer;">
                            <span>
                                <i class="fa-solid fa-arrows-rotate"></i> Refresh Captcha
                            </span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Login</button>
                </div>
            </div>
        </div>
    </div>
